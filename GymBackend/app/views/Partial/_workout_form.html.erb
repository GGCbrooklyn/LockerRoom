<%= form_for(@new_workout, html: {multipart: true}) do |f| %>
  <% render 'layouts/errors', object: f.object %>
  <%= f.label :goal %><br>
  <%= f.collection_select(:goal_id, @goals, :id, Exercise.find(goal.exercise_id).name, :include_blank => "Please Select") %>

  <%= f.number_field :reps, placeholder: 'Reps' %><br>
  <%= f.number_field :weight, placeholder: 'Weight' %><br>
  <%= f.submit%>
<% end %>